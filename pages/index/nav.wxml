<view class="fui-icon-group col-4 noborder circle" wx:if="{{item.data.length>0}}">
    <navigator class="fui-icon-col" openType="navigate" url="{{item.url}}" wx:for="{{item.data}}" wx:for-index="idx">
        <view>
            <image src="{{item.icon}}"></image>
        </view>
        <view class="navs-navname">{{item.navname}}</view>
    </navigator>
</view>
